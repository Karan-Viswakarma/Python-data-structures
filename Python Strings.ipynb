   "source": [
    "def swap_case(s):\n",
    "    return \"\".join([i.upper() if i.islower() else i.lower() for i in s])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 12,
   "id": "d092c1f4",
   "metadata": {},
   "outputs": [],
   "source": [
    "def rev_swap_case(s):\n",
    "    l1 = s.split(\" \")\n",
    "    l2 = l1[::-1]\n",
    "    res = []\n",
    "    for i in l2:\n",
    "        res.append(swap_case(i))\n",
    "    res = \" \".join(res)\n",
    "    return res"
   ]
  },
